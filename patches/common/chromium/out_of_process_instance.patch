diff --git a/pdf/out_of_process_instance.cc b/pdf/out_of_process_instance.cc
index 25bb9fdd9b5c4cedccd401ca24d75807ddd77842..38987a29fdccbf1d4740779157b296cd4ea46cb2 100644
